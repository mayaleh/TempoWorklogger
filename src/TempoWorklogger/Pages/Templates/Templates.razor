@page "/templates"

<CascadingValue Name="@(nameof(TempoWorklogger.Contract.UI.ViewModels.Templates.ITemplatesViewModel))" Value="@vm">
    <TempoWorklogger.UI.Containers.ContentContainer IsLoading="@vm.IsInit" IsBusy="@vm.IsBusy">
        <StaticContent>
            <div class="row">
                <div class="col">
                    <h3 class="mb-2">Mapping Templates</h3>
                </div>
                <div class="col-auto">
                    <GoBack />
                </div>
            </div>
        </StaticContent>
        <Content>
            <TempoWorklogger.UI.Views.Templates.TemplatesView />
        </Content>
    </TempoWorklogger.UI.Containers.ContentContainer>
</CascadingValue>
